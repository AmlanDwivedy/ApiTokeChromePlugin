{
  "manifest_version": 2,
  "author": "Amlan Dwivedy",

  "name": "API TokenGrabber ",
  "version": "1.0.0",
  "description": "",

  "browser_action": {
    "default_title": "TokenGrabber",
    "default_popup": "ApiTokenPopup.html"
  },  

  "icons": { 
    "16": "key_16.png",
    "32": "key_32.png",
    "48": "key_64.png",
    "64": "key_64.png",
    "128": "key_128.png",
    "256": "key_256.png"
  },

  "permissions": [
      "webRequest", 
      "webRequestBlocking", 
      "http://*/*",
      "https://*/*",
      "tabs", 
      "storage"
   ]
}